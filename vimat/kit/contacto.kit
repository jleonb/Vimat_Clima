<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="keywords" content="">
    <meta name="description" content="">
    
    <!-- @include header.kit -->

    <script src='./js/jqBootstrapValidation.js'></script>
	<script src='./js/formHandler.js'></script>
	<script src="./js/jquery.flexisel.js"></script>
	<link rel='stylesheet' href='./css/flexisel.min.css'/>
    
    <title>Contacto - Vimat Clima</title>
</head>
<body>
<!-- Main container -->
<div class="page-container">
    
<!-- Navigation Bloc -->

	<!-- @include menu.kit -->

<!-- Navigation Bloc END -->

<!-- bloc-1 -->
<div class="bloc l-bloc bgc-white" id="bloc-1">
	<div class="container bloc-md">
		<div class="row">
			<div class="col-sm-6">
				<form id="form-1" novalidate>
					<div class="form-group">
						<label>
							<i class="fa fa-user fa-fw"></i> Nombre
						</label>
						<input id="name" class="form-control" required />
					</div>
					<div class="form-group">
						<label>
							<i class="fa fa-phone fa-fw"></i> Tel√©fono
						</label>
						<input id="tel" class="form-control" required data-validation-required-message="tel is required." />
					</div>
					<div class="form-group">
						<label>
							<i class="fa fa-envelope fa-fw"></i> Email
						</label>
						<input id="email" class="form-control" type="email" required />
					</div>
					<div class="form-group">
						<label>
							<i class="fa fa-pencil fa-fw"></i> Mensaje
						</label><textarea id="message" class="form-control" rows="4" cols="50" required></textarea>
					</div> 
					<button class="bloc-button btn btn-block btn-orange-color-wheel btn-sq" type="submit">
						ENVIAR
					</button>
				</form>
			</div>
			<div class="col-sm-6">
				<img src="./img/contacto.jpg" alt="contacto" class="foto_contacto img-responsive animated bounceInRight" />
			</div>
		</div>
	</div>
</div>
<!-- bloc-1 END -->

<!-- bloc-19 -->
<div class="bloc l-bloc bgc-white" id="bloc-19">
	<div class="container bloc-sm">
		<div class="row">
			<div class="col-sm-12">
				<ul id="logos">
				    <li><img src="http://placehold.it/150x100" /></li>
				    <li><img src="http://placehold.it/150x100" /></li>
				    <li><img src="http://placehold.it/150x100" /></li>
				    <li><img src="http://placehold.it/150x100" /></li>
				    <li><img src="http://placehold.it/150x100" /></li>
				    <li><img src="http://placehold.it/150x100" /></li>
				</ul>
			</div>
		</div>
	</div>
</div>
<!-- bloc-19 END -->

<!-- Footer - bloc-25 -->

	<!-- @include footer.kit -->

<!-- Footer - bloc-25 END -->
</div>
<!-- Main container END -->

</body>

<script type="text/javascript">
	$(window).load(function() {
	  $("#logos").flexisel({
	    visibleItems: 4,
	    animationSpeed: 1000,
	    autoPlay: true,
	    autoPlaySpeed: 3000,
	    pauseOnHover: true,
	    clone:true,
	    enableResponsiveBreakpoints: true,
	    responsiveBreakpoints: {
	      portrait: {
	        changePoint:480,
	        visibleItems: 1
	      }, 
	      landscape: {
	        changePoint:640,
	        visibleItems: 2
	      },
	      tablet: {
	        changePoint:768,
	        visibleItems: 3
	      }
	    }
	  });
});
</script>
    
